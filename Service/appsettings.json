{
  "https_port": 5001,
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Error",
      "Microsoft.EntityFrameworkCore": "Error",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {
    "Secret": "xxxxxx",
    "Issuer": "https://openlabyrinth.ca",
    "Audience": "https://dev.olab.ca",
    "WebsitePublicFilesDirectory": "<root>/static/files",
    "DefaultImportDirectory": "/tmp"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultDatabase": "server=<server>;uid=<user>;pwd=<password>;database=<database>"
  }
}
